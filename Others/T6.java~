import java.util.Scanne;
public class T6{
  public static void main(String args[]){
    Scanner sc = new Scanner(System.in);
    System.out.println("Enter the value of S");
    double s = sc.nextDouble();
    if(S<100){
      L = 3000-125*S*S;
      System.out.println(L)
    }
    else{
      L = 12000/(4+(S*S/14900));
      System.out.println(L);
    }
  }
}