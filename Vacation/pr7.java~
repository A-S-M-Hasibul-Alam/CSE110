import java.util.Scanner;
public class pr7{
  public static void main(String args[]){
    Scanner sc = new Scanner(System.in);
    System.out.println("Enter a number");
    int a = sc.nextInt();
    for(int n=1; n<=a; n++){
     for(int s=1; s<=2*n-1; s++){
        System.out.print(s);
      }
      System.out.println();
    }
  }
}
